<?xml version="1.0"?>

	<PropertyList>

	<path>light-left-panel.ac</path>

	<!-- collision-switch -->
	<model>
		<name>Colision lights</name>
    <path>Aircraft/F-15C/Models/Interior/Panel/Instruments/lights/colision-lights.xml</path>
    <offsets> 
      <x-m>-0.054</x-m>
      <y-m>-0.04</y-m>
      <z-m>0.0</z-m>
	  <pitch-deg>-90</pitch-deg>

    </offsets>
	</model>
	
	<animation>
    <type>select</type>
    <object-name>white-colision</object-name>
    <object-name>red-colision</object-name>
         
    <condition>
      <equals>
        <property>controls/switches/col-light</property>
        <value> 1 </value>
      </equals>
    </condition>
  </animation> 

	<!-- taxi/probe-switch -->
		<model>
		<name>Landing lights</name>
    <path>Aircraft/F-15C/Models/Interior/Panel/Instruments/lights/landing-lights.xml</path>
    <offsets> 
      <x-m>-0.015</x-m>
      <y-m>-0.04</y-m>
      <z-m>0.0</z-m>
	  <pitch-deg>-90</pitch-deg>

    </offsets>
	</model>
	
	<animation>
    <type>select</type>
    <object-name>NoseGearLandinglight</object-name>
    <object-name>Taxi-Light-Left</object-name>
         
    <condition>
      <equals>
        <property>controls/switches/landing-light</property>
        <value> 1 </value>
      </equals>
    </condition>
  </animation> 

